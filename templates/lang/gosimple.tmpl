{{- /* dot == lang.GoSimple */}}

{{define "lang/gosimple/definition"}}
    {{-  .Name }}
{{- end}}
{{define "lang/gosimple/usage"}}
    {{- if .RuntimeImport}}
        {{- goqualrun .Import .Name }}
    {{- else if .Import }}
        {{- goqual .Import .Name }}
    {{- else }}
        {{- gopkg . }}{{ .Name }}
    {{- end }}
{{- end}}