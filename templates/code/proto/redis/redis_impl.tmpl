{{define "code/proto/redis/server/impl/github.com/redis/go-redis/connectFunction"}}
func Connect{{ . | goID }}Bidi(
    ctx {{goPkgExt "context"}}Context,
    url *{{goPkgExt "net/url"}}URL,
    {{with $.SecuritySchemes}}security {{$ | goID}}Security,{{end}}
) (*{{ . | goID }}Closable, error) {
    client, err := {{goPkgImpl .Protocol}}NewClient(url.String(), {{if $.SecuritySchemes}}security{{else}}nil{{end}})
    if err != nil {
        return nil, err
    }
    producer, consumer := client, client
    return &{{ . | goID }}Closable{
        {{. | goID}}{producer: producer, consumer: consumer},
    }, nil
}
{{- end}}

