{{- /* dot == render.Operation */}}
{{- pin .}}

{{- if .Bindings }}
    type {{goID $}}Bindings struct{}

    {{- range $proto := $.Bindings.Protocols}}
        func (c {{goID $}}Bindings) {{ goID $proto }}() {{goPkgUtil $proto}}OperationBindings {
        {{- with tryTmpl (print "code/proto/" $proto "/operation/bindings/values") ($.ProtoOperation $proto)}}
            return {{.}}
        {{- else}}
            return {{goPkgUtil $proto}}OperationBindings{}
        {{- end}}
        }
    {{- end}}
{{- end}}