{{- /* dot == common.Artifact: Ref -> render.Operation or render.Operation */}}
{{- with $operation := deref .}}
{{- pin $operation}}

{{- with $bindingsType := once .BindingsType }}
    {{ $bindingsType | goDef }}
    {{- range $proto := $operation.BindingsProtocols }}
        {{- $bindingsValue := $operation.ProtoBindingsValue $proto }}
        func (c {{ $bindingsType | goUsage }}) {{ $proto | goIDUpper }}() {{$bindingsValue.Type | goUsage}} {
            b := {{$bindingsValue | goUsage}}
            {{- $jvals := $operation.Bindings.JSONValues.GetOrEmpty $proto }}
            {{- range $jvals.Entries}}
                {{.Key | toCamelCase }} := {{.Value | goLit}}
                _ = {{goQual "encoding/json.Unmarshal"}}([]byte({{.Key | toCamelCase }}), &b.{{.Key}})
            {{- end}}
            return b
        }
    {{- end}}
{{- end}}
{{- end}}